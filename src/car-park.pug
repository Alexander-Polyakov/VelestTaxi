                                a(href='./images/previews/preview4.jpg'  data-fancybox='gallery').image-proportional-box.image-proportional-box_gallery
                                    img(src='./images/previews/preview4.jpg').image-proportional-box__image
                            .gallery-grid__item
                                a(href='./images/previews/preview4.jpg'  data-fancybox='gallery').image-proportional-box.image-proportional-box_gallery
                                    img(src='./images/previews/preview4.jpg').image-proportional-box__image
                            .gallery-grid__item
                                a(href='./images/previews/preview4.jpg'  data-fancybox='gallery').image-proportional-box.image-proportional-box_gallery
                                    img(src='./images/previews/preview4.jpg').image-proportional-box__image

            include pug/components/footer


        include pug/scripts.pug

